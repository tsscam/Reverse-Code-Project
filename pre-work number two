ConsoleApplication
{
  class Program
  {
   static void Main(string[] args)
       {
        Console.WriteLine("Please type 5 numbers here : ");
        int num = int.Parse(Console.ReadLine());
        int reverse = 0;
        while (num > 0)
        {
        int remainder = num % 10;
        reverse = (reverse * 10) + remainder;
        num = num / 10;
        }
        Console.WriteLine("These 5 numbers reversed : {0}", reverse);
        Console.ReadKey();
      }
    }
  }
  
